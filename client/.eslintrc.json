{
    "env": {
        "browser": true,
        "node": true,
        "es2020": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": ["react", "no-comments"],
    "rules": {
        "no-console": "error",
        "no-comments/disallowComments": "off",
        "react/prop-types": "off"
    },
    "settings": {
        "react": {
            "createClass": "createReactClass",
            "pragma": "React",
            "fragment": "Fragment",
            "version": "detect",
            "flowVersion": "0.53"
        }
    }
}
